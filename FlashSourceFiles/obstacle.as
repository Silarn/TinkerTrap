package  {		import flash.display.MovieClip;			public class obstacle extends MovieClip {				public var xtile, ytile, depthshift, depth: Number;		public var speed: Number;		public var downY, upY, leftX, rightX: Number;		public var downYPos, upYPos, leftXPos, rightXPos: Number;		public var posX, posY, xiso, yiso: Number;		public var upleft, downleft, upright, downright: Boolean;		public var type, currDir: int;				public function obstacle(t: int) {			currDir = 3;			gotoAndStop(t);			type = t;		}				public function setFacing(frame: int) {			if (type == 1)				char.gotoAndStop(frame);		}	}}